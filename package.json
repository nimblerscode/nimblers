{
  "name": "nimblers",
  "version": "0.0.1",
  "description": "A RedwoodSDK starter for projects with a database using Drizzle",
  "main": "index.js",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "NODE_ENV=${NODE_ENV:-development} vite dev",
    "dev:init": "rw-scripts dev-init",
    "test": "vitest",
    "lint": "biome check ./src",
    "test:watch": "vitest --watch",
    "preview": "npm run build && npm run vite preview",
    "worker:run": "rw-scripts worker-run",
    "clean": "npm run clean:vite",
    "clean:vite": "rm -rf ./node_modules/.vite",
    "release": "npm run clean && npm run build && wrangler deploy",
    "format": "biome check --write ./src",
    "migrate:new": "drizzle-kit generate",
    "migrate:gateway": "drizzle-kit generate --config=drizzle.config.gateway.ts",
    "migrate:tenant": "drizzle-kit generate --config=drizzle.config.tenant.ts",
    "migrate:dev": "wrangler d1 migrations apply DB --local",
    "seed": "npm run worker:run ./src/infra/db/seed.ts",
    "seed:users": "npm run worker:run ./src/infra/db/seed.ts",
    "check": "npm run types",
    "types": "npm run tsc"
  },
  "dependencies": {
    "@effect/experimental": "^0.44.19",
    "@effect/platform": "^0.80.21",
    "@redwoodjs/sdk": "0.0.78",
    "better-auth": "^1.2.7",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "effect": "^3.14.19",
    "react": "19.2.0-canary-39cad7af-20250411",
    "react-dom": "19.2.0-canary-39cad7af-20250411",
    "resend": "^4.5.1",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@cloudflare/vitest-pool-workers": "^0.8.24",
    "@cloudflare/workers-types": "^4.20250505.0",
    "@effect/vitest": "^0.21.2",
    "@types/node": "^22.15.9",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^3.1.3",
    "drizzle-kit": "^0.31.1",
    "miniflare": "^4.20250428.1",
    "msw": "^2.7.6",
    "tsx": "^4.19.4",
    "uuid": "^11.1.0",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3",
    "wrangler": "^4.14.1"
  },
  "packageManager": "pnpm@10.10.0"
}
